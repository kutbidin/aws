{
    "compilerOptions": {
      "target": "es2020",
      "module": "commonjs",
      "lib": ["es2020"],
      "outDir": "dist",
      "rootDir": "src",
      "strict": true,
      "esModuleInterop": true,
      "sourceMap": true,
      "declaration": true,
      "noImplicitAny": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "forceConsistentCasingInFileNames": true      
    },
    "references":
     [
        { "path": "../typescript-http-api-models" }
     ],
    "extends": "../tsconfig.base.json",    
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
  }
  